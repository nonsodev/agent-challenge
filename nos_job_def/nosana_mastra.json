{
  "ops": [
    {
      "id": "Project-manager-agent",
      "args": {
        "gpu": true,
        "image": "docker.io/nonsodev/agent-challenge:latest",
        "expose": [
          {
            "port": 8080,
            "health_checks": [
              {
                "path": "/health",
                "type": "http",
                "method": "GET",
                "continuous": true,
                "expected_status": 200
              }
            ]
          }
        ],
        "entrypoint": ["/bin/sh"],
        "cmd": ["/start.sh"],
        "env": {
          "API_BASE_URL": "http://0.0.0.0:11434/api",
          "MODEL_NAME_AT_ENDPOINT": "qwen2.5:32b"
        }
      },
      "type": "container/run"
    }
  ],
  "meta": {
    "trigger": "dashboard",
    "system_requirements": {
      "required_vram": 12
    },
    "description": "AI project partner that reads your messy boards, fixes the chaos, and creates winning roadmapsâ€”proactively."
  },
  "type": "container",
  "version": "0.1"
}